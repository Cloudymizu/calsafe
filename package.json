{
  "name": "calsafejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prettier --write . && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --log-level warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "modern-normalize": "^3.0.1",
    "next": "^14.2.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "20.16.13",
    "@types/react": "18.3.11",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "<5.5.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix",
    "*.{json,js,ts,jsx,tsx,html,md,css}": "prettier --write"
  }
}
